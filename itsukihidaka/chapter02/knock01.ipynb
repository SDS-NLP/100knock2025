   "source": [
    "# ファイルの先頭N行だけを表示\n",
    "def read_N(N):\n",
    "    with open('popular-names.txt', 'r') as f:\n",
    "        for _ in range(N):\n",
    "            print(f.readline(),end='')\n",
    "\n",
    "read_N(10)"
   ]
  },
